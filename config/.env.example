# A2A Gateway Environment Variables
# Copy to .env and fill in your values
# NEVER commit .env files with actual values

# Environment (dev, staging, prod)
ENVIRONMENT=dev

# Databricks Configuration (REQUIRED)
DATABRICKS_HOST=https://adb-XXXXXXXXX.XX.azuredatabricks.net
DATABRICKS_WORKSPACE_ID=XXXXXXXXX

# Entra ID Configuration (REQUIRED)
ENTRA_TENANT_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Optional: Entra App Registration for Foundry/Copilot OAuth
ENTRA_CLIENT_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Azure Configuration
AZURE_LOCATION=eastus2
RESOURCE_PREFIX=a2a

# APIM Configuration (REQUIRED)
APIM_PUBLISHER_EMAIL=admin@example.com
APIM_PUBLISHER_NAME=A2A Gateway
APIM_SKU=Consumption

# Test Configuration
TEST_AGENT_NAME=echo
RESTRICTED_AGENT_NAME=restricted
